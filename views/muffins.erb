<h2>My Muff Muffs</h2>
  <div class="row">
<% muffins.each do |muffin| %>
    <div class="col s12 m6 l3">
      <div class="card">
        <div class="card-image">
          <img src="./muffins/<%= muffin.ima_url%>">
          <span class="card-title"><%= muffin.name %></span>
        </div>
        <div class="card-content">
          <p><%= muffin.description%></p>
        </div>
        <div class="card-action">
          <p class="price">$<%= muffin.price %> </p>
        </div>
      </div>
    </div>
<%end %>
  </div>
